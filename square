import cv2

square_cascade = cv2.CascadeClassifier("haarcascade_russian_plate_number.xml")
img=cv2.VideoCapture("4K Video of Russian highway traffic.mp4")
while True:
    ret, frames = img.read()
    gray=cv2.cvtColor(frames, cv2.COLOR_BGR2GRAY)
    cars=square_cascade.detectMultiScale(gray, 1.1, 1)

    for(x, y, w, h) in cars:
        cv2.rectangle(frames, (x,y),(x+w,y+h),(0,0,255),2)

    cv2.imshow("video2", frames)

    if cv2.waitKey(33)==27:
        break

cv2.destroyAllWindows()
